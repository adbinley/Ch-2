xlab("Season") +
ylab("Mean MHA") +
theme(axis.text.x = element_text(angle = 45, hjust=1))+
scale_color_manual(name = "Migratory Strategy",
labels = c("Neotropical Migrants","Short-Distance Migrants","Neotropical Residents","North American Residents"),
